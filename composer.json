{
    "name": "dmelearn/crawler",
    "description": "Crawl all internal links found on a website. Originally based on spatie/crawler 2.x",
    "homepage": "https://github.com/ajdunn2/crawler",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "symfony/dom-crawler": "^4.0|^5.0",
        "guzzlehttp/guzzle": "^6.5.8|^7.4.5",
        "tightenco/collect": "^6.0",
        "nicmart/tree": "^0.3.1",
        "spatie/browsershot": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Dmelearn\\Crawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmelearn\\Crawler\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
